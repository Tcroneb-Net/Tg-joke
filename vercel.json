{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/joke", "dest": "/api/joke" },
    { "src": "/anime", "dest": "/api/anime" },
    { "src": "/clone", "dest": "/api/clone" },
    { "src": "/loop", "dest": "/api/loop" },
    { "src": "/meme", "dest": "/api/meme" },
    { "src": "/dns", "dest": "/api/dns" },
    { "src": "/gpt", "dest": "/api/gpt" }
  ]
}
